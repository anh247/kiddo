{% liquid
    assign style_file = 'global.css'
    if settings.rtl
        assign style_file = 'global-rtl.css'
    endif
    assign rtlCountries = settings.rtl_code | downcase | split: ' '
    for code in rtlCountries
        assign code = code | strip
        if shop.locale == code
            assign style_file = 'global-rtl.css'
        endif
    endfor
%}
<link rel="preconnect" href=" {{ style_file | asset_url }}" crossorigin>
<link rel="preload" as="style" href="{{ style_file | asset_url }}">
{{ style_file | asset_url | stylesheet_tag }}

{% if settings.custom_css %}
    <link rel="preconnect" href=" {{ 'custom.css' | asset_url }}" crossorigin>
    <link rel="preload" as="style" href="{{ 'custom.css' | asset_url }}">
    {{ 'custom.css' | asset_url | stylesheet_tag }}
{% endif %}

{%- style -%}
    {%- capture style -%}
        :root{
        /*================ Main ================*/
        {% if settings.container != blank %}--container:{{ settings.container }};{% endif %}
        {% if settings.sidebar_width != blank %}--sidebar-width:{{ settings.sidebar_width }};{% endif %}
        {% if settings.sidebar_space != blank %}--sidebar-space:{{ settings.sidebar_space }};{% endif %}
        {% if settings.body_fz != blank %}--main-fz:{{ settings.body_fz }};{% endif %}
        {% if settings.body_lh != blank %}--main-lh:{{ settings.body_lh }};{% endif %}
        {% if settings.body_ls != blank %}--main-ls:{{ settings.body_ls }};{% endif %}
        {% if settings.body_fw != blank %}--main-fw:{{ settings.body_fw }};{% endif %}
        {% if settings.body_tt != blank %}--main-tt:{{ settings.body_tt }};{% endif %}
        {% if settings.bd_text_space != blank %}--text-space:{{ settings.bd_text_space }};{% endif %}
        {% if settings.bd_radius != blank %}--main-bora:{{ settings.bd_radius }};{% endif %}
        {% if settings.bd_radius_2 != blank %}--main-bora-2:{{ settings.bd_radius_2 }};{% endif %}
        {% if settings.heading_ls != blank %}--heading-ls:{{ settings.heading_ls }};{% endif %}
        {% if settings.heading_fw != blank %}--heading-fw:{{ settings.heading_fw }};{% endif %}
        {% if settings.heading_fw_2 != blank %}--heading-fw-2:{{ settings.heading_fw_2 }};{% endif %}
        {% if settings.heading_tt != blank %}--heading-tt:{{ settings.heading_tt }};{% endif %}
        {% if settings.heading_space != blank %}--heading-space:{{ settings.heading_space }};{% endif %}
        {% if settings.input_bd_width != blank %}--input-bd-width:{{ settings.input_bd_width }};{% endif %}
        {% if settings.input_bd_radius != blank %}--input-bora:{{ settings.input_bd_radius }};{% endif %}
        {% if settings.btn_bd_width != blank %}--btn-bd-width:{{ settings.btn_bd_width }};{% endif %}
        {% if settings.btn_bd_radius != blank %}--btn-bora:{{ settings.btn_bd_radius }};{% endif %}
        {% assign main_cl_rgb = settings.main_cl | color_to_rgb | remove_first: 'rgb(' | remove: ')' | replace: ' ', '' | split: ',' %}
        {% assign dark_cl_rgb = settings.dark_cl | color_to_rgb | remove_first: 'rgb(' | remove: ')' | replace: ' ', '' | split: ',' %}
        {% assign light_cl_rgb = settings.light_cl | color_to_rgb | remove_first: 'rgb(' | remove: ')' | replace: ' ', '' | split: ',' %}
        {% assign main_cl_hsl = settings.main_cl | color_to_hsl | remove_first: 'hsl(' | remove: ')' | replace: ' ', '' | split: ',' %}
        {% assign dark_cl_hsl = settings.dark_cl | color_to_hsl | remove_first: 'hsl(' | remove: ')' | replace: ' ', '' | split: ',' %}
        {% assign light_cl_hsl = settings.light_cl | color_to_hsl | remove_first: 'hsl(' | remove: ')' | replace: ' ', '' | split: ',' %}
        /*================ Root ================*/
        --main-cl:{{ settings.main_cl }};
        --main-cl-r:{{ main_cl_rgb[0] | default: 255 }};
        --main-cl-g:{{ main_cl_rgb[1] | default: 255 }};
        --main-cl-b:{{ main_cl_rgb[2] | default: 255 }};
        --main-cl-h:{{ main_cl_hsl[0] | default: 0 }};
        --main-cl-s:{{ main_cl_hsl[1] | default: '0%' }};
        --main-cl-l:{{ main_cl_hsl[2] | default: '0%' }};
        --main-cl-t:{{ settings.main_cl_t }};
        --dark-cl:{{ settings.dark_cl }};
        --dark-cl-r:{{ dark_cl_rgb[0] | default: 255 }};
        --dark-cl-g:{{ dark_cl_rgb[1] | default: 255 }};
        --dark-cl-b:{{ dark_cl_rgb[2] | default: 255 }};
        --dark-cl-h:{{ dark_cl_hsl[0] | default: 0 }};
        --dark-cl-s:{{ dark_cl_hsl[1] | default: '0%' }};
        --dark-cl-l:{{ dark_cl_hsl[2] | default: '0%' }};
        --dark-cl-2:{{ settings.dark_cl_2 }};
        --dark-cl-3:{{ settings.dark_cl_3 }};
        --light-cl:{{ settings.light_cl }};
        --light-cl-r:{{ light_cl_rgb[0] | default: 255 }};
        --light-cl-g:{{ light_cl_rgb[1] | default: 255 }};
        --light-cl-b:{{ light_cl_rgb[2] | default: 255 }};
        --light-cl-h:{{ light_cl_hsl[0] | default: 0 }};
        --light-cl-s:{{ light_cl_hsl[1] | default: '0%' }};
        --light-cl-l:{{ light_cl_hsl[2] | default: '0%' }};
        --light-cl-2:{{ settings.light_cl_2 }};
        --line-cl:{{ settings.line_cl }};
        {% if settings.body_custom_cl %}
            --body-cl:{{ settings.body_cl }};
            --body-bg:{{ settings.body_bg }};
        {% endif %}
        {% if settings.link_custom_cl %}
            --link-cl:{{ settings.link_cl }};
            --link-cl-hover:{{ settings.link_cl_hover }};
        {% endif %}
        {% if settings.input_custom_cl %}
            --input-cl:{{ settings.input_cl }};
            --input-bg:{{ settings.input_bg }};
            --input-bd:{{ settings.input_bd }};
            --input-cl-hover:{{ settings.input_cl_hover }};
            --input-bg-hover:{{ settings.input_bg_hover }};
            --input-bd-hover:{{ settings.input_bd_hover }};
        {% endif %}
        {% if settings.btn_custom_cl %}
            --btn-cl:{{ settings.btn_cl }};
            --btn-bg:{{ settings.btn_bg }};
            --btn-bd:{{ settings.btn_bd }};
            --btn-cl-hover:{{ settings.btn_cl_hover }};
            --btn-bg-hover:{{ settings.btn_bg_hover }};
            --btn-bd-hover:{{ settings.btn_bd_hover }};
        {% endif %}
        /*================ Post ================*/
        --post-title-line:{{ settings.post_title_line }};
        {% if settings.post_title_equal %}--post-title-height:calc(var(--post-title-line) * var(--post-title-lh));{% endif %}
        --post-excerpt-line:{{ settings.post_excerpt_line }};
        /*================ Product ================*/
        --prd-title-line:{{ settings.prd_title_line }};
        {% if settings.prd_title_equal %}--prd-title-height:calc(var(--prd-title-line) * var(--prd-title-lh));{% endif %}
        --prd-excerpt-line:{{ settings.prd_excerpt_line }};
        {% if settings.prd_custom_cl %}
            --prd-title-cl:{{ settings.prd_title_cl }};
            --prd-title-cl-hover:{{ settings.prd_title_cl_hover }};
        {% endif %}
        --label-cl-text:{{ settings.label_cl_text }};
        --label-hot-cl:{{ settings.label_hot_cl }};
        --label-new-cl:{{ settings.label_new_cl }};
        --label-sale-cl:{{ settings.label_sale_cl }};
        --in-stock-cl:{{ settings.in_stock_cl }};
        --out-stock-cl:{{ settings.out_stock_cl }};
        }

        {% if settings.sidebar_width_1500 != blank %}@media (max-width: 1499px){:root{--sidebar-width-1500:{{ settings.sidebar_width_1500 }};}}{% endif %}
        {% if settings.sidebar_space_1500 != blank %}@media (max-width: 1499px){:root{--sidebar-space-1500:{{ settings.sidebar_space_1500 }};}}{% endif %}

        .hero_canvas:before,
        .mfp-container,.mask-overlay{
        cursor: url({{ 'cursor-close.png' | asset_url }}),pointer !important;
        }
        .content-video .number:before{
        -webkit-mask: url({{ 'icon_1.svg' | asset_url }}) no-repeat center center;
        mask: url({{ 'icon_1.svg' | asset_url }}) no-repeat center center;
        }
    {%- endcapture -%}
    {{ style | strip_newlines }}
{%- endstyle -%}
